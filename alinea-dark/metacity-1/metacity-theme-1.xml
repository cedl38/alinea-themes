<frame_style name="normal" geometry="normal">
	<piece position="entire_background" draw_ops="bevel_n"/>
	<piece position="title" draw_ops="title_n"/>
 	<piece position="overlay" draw_ops="edge_n"/>
	<button function="menu" state="normal" draw_ops="menu_icon_n"/>
	<button function="minimize" state="normal" draw_ops="minimize_icon_n"/>
	<button function="maximize" state="normal" draw_ops="maximize_icon_n"/>
	<button function="close" state="normal" draw_ops="close_icon_n"/>

	<button function="menu" state="pressed" draw_ops="menu_icon_p"/>
	<button function="menu" state="prelight" draw_ops="menu_icon_p"/>
	<button function="minimize" state="pressed" draw_ops="minimize_icon_p"/>
	<button function="minimize" state="prelight" draw_ops="minimize_icon_p"/>
	<button function="maximize" state="pressed" draw_ops="maximize_icon_p"/>
