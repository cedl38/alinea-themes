	<button function="close" state="pressed" draw_ops="close_icon_p"/>
	<button function="close" state="prelight" draw_ops="close_icon_p"/>
</frame_style>

<frame_style name="focused" geometry="normal" parent="normal">
	<piece position="entire_background" draw_ops="bevel_f"/>
	<piece position="title" draw_ops="title_f"/>
 	<piece position="overlay" draw_ops="edge_f"/>
	<button function="menu" state="normal" draw_ops="menu_icon_f"/>
