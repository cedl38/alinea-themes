	<button function="menu" state="pressed" draw_ops="menu_icon_p"/>
	<button function="maximize" state="pressed" draw_ops="maximize_icon_p"/>
	<button function="minimize" state="pressed" draw_ops="minimize_icon_p"/>
	<button function="close" state="pressed" draw_ops="close_icon_p"/>
	<button function="menu" state="prelight" draw_ops="menu_icon_p"/>
	<button function="maximize" state="prelight" draw_ops="maximize_icon_p"/>
	<button function="minimize" state="prelight" draw_ops="minimize_icon_p"/>
	<button function="close" state="prelight" draw_ops="close_icon_p"/>
</frame_style>

