	<piece position="entire_background" draw_ops="bevel_utility_f"/>
 	<piece position="overlay" draw_ops="blank"/>
</frame_style>

<frame_style_set name="normal">
	<frame focus="yes" state="normal" resize="both" style="focused"/>
	<frame focus="no" state="normal" resize="both" style="normal"/>
	<frame focus="yes" state="maximized" style="focused_max"/>
	<frame focus="no" state="maximized" style="normal_max"/>
	<frame focus="yes" state="shaded" style="focused"/>
	<frame focus="no" state="shaded" style="normal"/>
	<frame focus="yes" state="maximized_and_shaded" style="focused_max"/>
	<frame focus="no" state="maximized_and_shaded" style="normal_max"/>
</frame_style_set>

<frame_style_set name="utility">
	<frame focus="yes" state="normal" resize="both" style="utility_f"/>
	<frame focus="no" state="normal" resize="both" style="utility_n"/>
	<!-- this is a bunch of crack since utility windows shouldn't be maximized -->
	<frame focus="yes" state="maximized" style="focused"/>
	<frame focus="no" state="maximized" style="normal"/>
	<frame focus="yes" state="shaded" style="utility_f"/>
	<frame focus="no" state="shaded" style="utility_n"/>
	<frame focus="yes" state="maximized_and_shaded" style="focused"/>
	<frame focus="no" state="maximized_and_shaded" style="normal"/>
</frame_style_set>

<frame_style_set name="border">
	<frame focus="yes" state="normal" resize="both" style="border_f"/>
	<frame focus="no" state="normal" resize="both" style="border_n"/>
	<frame focus="yes" state="maximized" style="border_f"/>
	<frame focus="no" state="maximized" style="border_n"/>
	<frame focus="yes" state="shaded" style="border_f"/>
	<frame focus="no" state="shaded" style="border_n"/>
	<frame focus="yes" state="maximized_and_shaded" style="border_f"/>
	<frame focus="no" state="maximized_and_shaded" style="border_n"/>
</frame_style_set>

<window type="normal" style_set="normal"/>
<window type="dialog" style_set="normal"/>
<window type="modal_dialog" style_set="normal"/>
