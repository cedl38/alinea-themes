	<button function="maximize" state="prelight" draw_ops="restore_icon_p"/>
</frame_style>

<frame_style name="border_n" geometry="border" parent="normal">
	<piece position="entire_background" draw_ops="rectangle_border_n"/>
	<piece position="title" draw_ops="blank"/>
</frame_style>

<frame_style name="border_f" geometry="border" parent="normal">
	<piece position="entire_background" draw_ops="rectangle_border_f"/>
</frame_style>

<frame_style name="utility_n" geometry="utility" parent="normal">
	<piece position="entire_background" draw_ops="bevel_utility_n"/>
